{
    "name": "conectaqr",
    "version": "2.1.0",
    "type": "module",
    "main": "server.js",
    "private": true,
    "engines": {
        "node": ">=21.0.0"
    },
    "scripts": {
        "start": "node dist/server.js",
        "dev": "npx nodemon",
        "dev:silent": "npx nodemon",
        "build": "rm -rf ./dist && tsc",
        "lint": "eslint ./src",
        "test": "npx jest --verbose "
    },
    "keywords": [],
    "author": "AndreXimenes",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "@prisma/client": "^6.10.0",
        "argon2": "^0.43.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.2",
        "multer": "^2.0.1",
        "pg": "^8.16.1",
        "sharp": "^0.34.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.29.0",
        "@types/cookie-parser": "^1.4.9",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/multer": "^1.4.13",
        "@types/node": "^24.0.3",
        "@types/supertest": "^6.0.3",
        "eslint": "^9.29.0",
        "jest": "^30.0.0",
        "jest-extended": "^6.0.0",
        "nodemon": "^3.1.10",
        "prisma": "^6.10.0",
        "supertest": "^7.1.1",
        "ts-jest": "^29.4.0",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.34.1"
    },
    "jest": {
        "preset": "ts-jest/presets/default-esm",
        "globalSetup": "<rootDir>/tests/config/jest.setup.ts",
        "globalTeardown": "<rootDir>/tests/config/jest.teardown.ts",
        "testEnvironment": "node",
        "extensionsToTreatAsEsm": [
            ".ts"
        ],
        "transform": {
            "^.+\\.(t|j)sx?$": [
                "ts-jest",
                {
                    "tsconfig": "tests/tsconfig.test.json",
                    "useESM": true
                }
            ]
        },
        "setupFilesAfterEnv": [
            "jest-extended/all"
        ]
    }
}
